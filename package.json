{
  "name": "mobile-subcriber",
  "version": "1.0.0",
  "description": "This is a web rest api to assign client to a phone number ",
  "main": "app.js",
  "scripts": {
    "start": "node backend/app.js",
    "build:client": "npm run build --prefix frontend",
    "client": "npm start --prefix frontend",
    "server": "nodemon ./backend/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Abdul-Rauf Alhassan",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "google-auth-library": "^7.14.1",
    "mongoose": "^6.2.8",
    "phone": "^3.1.14",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
